<h1>User data</h1>
<div class = 'col-lg-12 col-md-12 row mb-3 me-0' >
  </div>
  <table class='table table-striped table-hover pretty'>
    <thead class='mb-2 bg-success'>
        <tr>

      </tr>
    </thead>
    <tbody>
      <% @user_application.each do |user| %>
        <tr>
          <td><%= user.id%></td>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.email %></td>
          <td><%= user.Gender %></td>
          <td><%= user.agree %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
