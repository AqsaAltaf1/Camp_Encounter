<div class="container", class="signout">

<div class="col-md-6 mx-auto">

  <h2>Sign up</h2>
  <p>Take a momenet to create a account with us</p><hr>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
          <div class="row">
           <div class="col">
             <%= f.label :FirstName %><br />
             <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name", class: "form-control"%>
          <span class="text-danger"><%= f.object.errors[:first_name][0] if f.object.errors.any? %></span>
           </div>
            <div class="col">

              <%= f.label :LastName %><br />
              <%= f.text_field :last_name, autofocus: true, autocomplete: "LastName", class: "form-control" %>
               <span class="text-danger"><%= f.object.errors[:last_name][0] if f.object.errors.any? %></span>
             </div>

          </div>

      <div class="row">

        <div class="col">
          <%= f.label :email%><br />
          <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control" %>
           <span class="text-danger"><%= f.object.errors[:email][0] if f.object.errors.any? %></span>
        </div>
        <div class="col">
          <%= f.label :Country%><br />
          <%= f.text_field :country, autofocus: true, autocomplete: "Country", class: "form-control" %>
           <span class="text-danger"><%= f.object.errors[:country][0] if f.object.errors.any? %></span>
        </div>

      </div>

      <div class="row">
        <div class="col">
          <%= f.label :PhoneNumber %><br />
          <%= f.text_field :phone_number, autofocus: true, autocomplete: "PhoneNumber", class: "form-control" %>
           <span class="text-danger"><%= f.object.errors[:phone_number][0] if f.object.errors.any? %></span>

        </div>
         <div class="col">
          <%= f.label :Type %><br />
          <select class="form-control" id="selecthere">
          <option>Client</option>
          <option>Admin</option>
          <option>SuperAdmin</option>
          </select>
           <span class="text-danger"><%= f.object.errors[:type][0] if f.object.errors.any? %></span>

        </div>

        <div class="form-group">
  <label for="exampleFormControlSelect1">Example select</label>

</div>



      </div>
      <div class="row">
        <div class="col">
          <%= f.label :password %>
          <% if @minimum_password_length %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
          <% end %><br />
          <%= f.password_field :password, autocomplete: "new-password", class: "form-control" %>
           <span class="text-danger"><%= f.object.errors[:password][0] if f.object.errors.any? %></span>
        </div>

        <div class="col">
          <%= f.label :password_confirmation %><br />
          <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "form-control" %>
           <span class="text-danger"><%= f.object.errors[:password_confirmation][0] if f.object.errors.any? %></span>
        </div>
      </div>

      <%= check_box_tag(:i_agree) %>
      <%= label_tag(:i_agree, "I agree") %><br />
      <%= check_box_tag(:not_agree) %>
      <%= label_tag(:not_agree, "I don't agree") %>

      <div class="row">
      <div class="col">
        <%= f.submit "Back to signin" %>
      </div>

      <div class="col">
        <%= f.submit "Sign up" %>
      </div>
      </div>

    <% end %>
      <%= render "users/shared/links" %>
    </div>



 </div>
