<div class='row mt-5'>
  <% if current_user&.admin? %>
    <h1>Welcome Admin </h1>
  <% elsif current_user&.client? && current_user.location_id.blank? %>
    <h1>Welcome  <%= current_user&.first_name%> <%= current_user&.last_name%> </h1>
  <% elsif current_user&.client? && current_user.location_id.present? %>
  <%= link_to 'View Application'%>
  <%#= current_user&.first_name%> <%#= current_user&.last_name%>
  <% else %>
    <h1> Welcome</h1>
  <% end %>
</div>
