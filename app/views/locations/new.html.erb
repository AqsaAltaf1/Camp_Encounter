<h1>New location</h1>

<%= form_with model: @location do |form| %>
  <div>
    <%= form.label :title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :camp_Location %><br>
    <%= form.text_area :camp_location %>
  </div>
  <div>
    <%= form.label :Status %><br>
    <%= form.text_area :status %>
  </div>
  <div>
    <%= form.label :start_date %><br>
     <%= form.datetime_field :start_date %>
  </div>
    <div>
    <%= form.label :end_date %><br>
     <%= form.datetime_field  :end_date %>
  </div>
  <div>
  <%= form.submit %>
  </div>
<% end %>
